
{
    "name": "Turntable Junk",
    "description": "does things on turntable.fm",
    "version": "0.1",
    "permissions": ["tabs", "*://*.turntable.fm/*"],
	
    "content_scripts": [
        {
        "matches": [
            "*://*.turntable.fm/*"
            ],
        "js": ["file.js"],
        "run_at": "document_end"    // Pay attention to this line
        }
    ], 
    "manifest_version": 2
}